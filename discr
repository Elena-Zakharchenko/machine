#include <iostream>
#include <cmath>

int main()
{
    float a, b, c, x1, x2, discr;
    std::cout << "Введите коэффициенты a, b и c: ";
    std::cin >> a >> b >> c;
    discr = b*b - 4*a*c;
    
    if (discr > 0)
    {
        x1 = (-b + sqrt(discr)) / (2*a);
        x2 = (-b - sqrt(discr)) / (2*a);
        std::cout << "x1 = " << x1 << "\n";
        std::cout << "x2 = " << x2 << "\n";
    }
    
    else if (discr == 0)
    {
        x1 = (-b + sqrt(discr)) / (2*a);
        std::cout << "x1 = " << x1;
    }
    return 0;
}
